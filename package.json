{
  "name": "gatsby-starter-default",
  "private": true,
  "description": "A simple starter to get up and developing quickly with Gatsby",
  "version": "0.1.0",
  "author": "Kyle Mathews <mathews.kyle@gmail.com>",
  "dependencies": {
    "@emotion/core": "^10.0.28",
    "core-js": "^3",
    "gatsby": "^2.21.21",
    "gatsby-image": "^2.4.3",
    "gatsby-plugin-emotion": "^4.3.1",
    "gatsby-plugin-manifest": "^2.4.2",
    "gatsby-plugin-offline": "^3.2.1",
    "gatsby-plugin-react-helmet": "^3.3.1",
    "gatsby-plugin-sharp": "^2.6.2",
    "gatsby-plugin-typography": "^2.5.1",
    "gatsby-source-filesystem": "^2.3.1",
    "gatsby-transformer-remark": "^2.8.7",
    "gatsby-transformer-sharp": "^2.5.2",
    "prop-types": "^15.7.2",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-helmet": "^6.0.0",
    "react-typography": "^0.16.19",
    "typography": "^0.16.19",
    "typography-theme-kirkham": "^0.16.19",
    "typography-theme-stow-lake": "^0.16.19"
  },
  "devDependencies": {
    "gh-pages": "^2.2.0",
    "prettier": "^2.0.5",
    "ts-pnp": "^1.2.0"
  },
  "keywords": [
    "gatsby"
  ],
  "license": "MIT",
  "scripts": {
    "build": "gatsby build",
    "develop": "gatsby develop",
    "format": "prettier --write src/**/*.{js,jsx}",
    "start": "yarn run develop",
    "serve": "gatsby serve",
    "test": "echo \"Write some tests! -> https://gatsby.dev/unit-testing\"",
    "deploy": "yarn run deploy-gh-pages && yarn run deploy-gh-netlify",
    "deploy-gh-pages": "gatsby build --prefix-paths && gh-pages --dist public --branch gh-pages",
    "deploy-gh-netlify": "git add . && git commit -a -m 'deploy' && git push",
    "netlify-only": "yarn run build && yarn run deploy-netlify",
    "new-p": "cp src/blog/create-template.md"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/gatsbyjs/gatsby-starter-default"
  },
  "bugs": {
    "url": "https://github.com/gatsbyjs/gatsby/issues"
  }
}
